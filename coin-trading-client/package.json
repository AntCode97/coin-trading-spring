{
  "name": "coin-trading-client",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:copy": "npm run build && mkdir -p ../coin-trading-server/src/main/resources/static && cp -r dist/* ../coin-trading-server/src/main/resources/static/",
    "desktop:dev": "concurrently -k \"cross-env VITE_DESKTOP_MODE=true VITE_API_BASE_URL=http://183.101.185.112:1104/api vite\" \"wait-on http://localhost:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "desktop:build:web": "cross-env VITE_DESKTOP_MODE=true VITE_API_BASE_URL=http://183.101.185.112:1104/api npm run build",
    "desktop:start": "cross-env ELECTRON_START_URL=file://$PWD/dist/index.html electron .",
    "desktop:dmg": "npm run desktop:build:web && electron-builder --mac dmg --publish never",
    "guard:public-surface": "node scripts/check-public-surface.mjs"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "html2canvas": "^1.4.1",
    "lightweight-charts": "^5.0.9",
    "openai": "^6.22.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^34.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.ant.cointrading.desktop",
    "productName": "CoinTradingWorkspace",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "identity": null,
      "hardenedRuntime": false,
      "gatekeeperAssess": false
    }
  }
}
